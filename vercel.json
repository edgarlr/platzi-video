{
  "version": 2,
  "builds": [
      {
          "src": "package.json",
          "use": "@vercel/statick-build"
      }
  ],
  "routes": [
    {
      "src": "(.*).js", "dest": "/$1"
    },
    {
      "src": "(.*).json", "dest": "/$1.json"
    },
    {
      "src": "(.*).png", "dest": "/$1.png"
    },
    {
      "src": "/.*", "dest": "/index.html"
    }
  ]
}