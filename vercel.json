{
  "version": 2,
  "builds": [{
    "src": "api/index.js",
    "use": "@vercel/node"
  },
  { 
    "use": "@now/static-build",
    "src": "package.json"
  }],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "api/index.js"
    },
    {
      "src": "(.*).js", "dest": "/$1"
    },
    {
      "src": "(.*).json", "dest": "/$1.json"
    },
    {
      "src": "(.*).png", "dest": "/$1.png"
    }
  ]
}