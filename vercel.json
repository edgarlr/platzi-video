{
  "version": 2,
  "builds": [{
    "src": "api/index.js",
    "use": "@vercel/node"
  },
  {
    "src": "src/**",
    "use": "@vercel/static"
  }],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "api/index.js"
    },
    {
      "src": "/",
      "dest": "public/index.html"
    },
    {
      "src": "/(.+)",
      "dest": "src/$1"
    }
  ]
}